{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "**/testing/*"
  ],
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictMetadataEmit": false,
    "skipTemplateCodegen": false,
    "preserveWhitespaces": false
  }
}
